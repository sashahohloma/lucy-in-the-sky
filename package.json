{
  "name": "lucy-in-the-sky",
  "version": "1.0.0",
  "description": "Test service",
  "main": "build/index.js",
  "scripts": {
    "bundle:run": "node build/index.js",
    "bundle:clear": "rimraf build",
    "bundle:compile": "npm run bundle:clear && tsc",
    "dev:live": "nest start --watch",
    "dev:watch": "tsc --watch --noEmit",
    "dev:test": "jest",
    "dev:check": "tsc --noEmit",
    "dev:lint": "eslint ./source --ext .ts",
    "dev:format": "npm run dev:lint -- --fix",
    "dev:upgrade": "npm install $(npm outdated | cut -d' ' -f 1 | sed '1d' | xargs -I '$' echo '$@latest' | xargs echo)",
    "migrate:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./source/database/migrations -d ./source/datasource.ts",
    "migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./source/datasource.ts",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./source/datasource.ts",
    "seeder:run": "ts-node ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d ./source/datasource.ts ./source/database/seeders/*.seeder.ts"
  },
  "keywords": [],
  "author": "Alexander Smirnov",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "10.2.1",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.2.1",
    "@nestjs/platform-fastify": "10.2.1",
    "@nestjs/typeorm": "10.0.0",
    "bytes": "^3.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "mysql2": "3.6.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@jest/globals": "29.6.4",
    "@jorgebodega/typeorm-seeding": "6.0.1",
    "@nestjs/cli": "10.1.15",
    "@nestjs/testing": "10.2.1",
    "@types/bytes": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
